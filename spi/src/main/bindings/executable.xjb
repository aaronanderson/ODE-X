<?xml version="1.0" encoding="UTF-8"?>
<jxb:bindings xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" version="2.0">
	<jxb:bindings schemaLocation="../resources/META-INF/xsd/executable.xsd">
		<jxb:schemaBindings>
			<jxb:package name="org.apache.ode.spi.exec.xml" />
		</jxb:schemaBindings>

		<jxb:bindings node="//xs:complexType[@name='executableType']">
			<jxb:class name="Executable" />
		</jxb:bindings>

	</jxb:bindings>
</jxb:bindings>